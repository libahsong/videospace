mixin video(video)
    a(href=`/videos/${video.id}`).video-mixin   
        .video-mixin__thumb(style=`background-image:url(${video.thumbUrl});background-size:cover;`)
        .video-mixin__data 
            sapn.video-mixin__title=video.title
            .video-mixin__meta
                span #{video.owner.name}·
                span #{video.meta.views}회